{
  // 使用 IntelliSense 了解相关属性。
  // 悬停以查看现有属性的描述。
  // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug-file",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "showGlobalVariables": true,
      "internalConsoleOptions": "openOnSessionStart",
      "program": "${file}",
    },
    {
      "name": "attach-server",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "showGlobalVariables": true,
      "internalConsoleOptions": "openOnSessionStart",
      "processId": "${command:pickGoProcess}",
    },
    {
      "name": "connect-server",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "remotePath": "${workspaceFolder}",
      "port": 8080,
      "host": "127.0.0.1",
      "showGlobalVariables": true,
      "internalConsoleOptions": "openOnSessionStart",
    }
  ]
}
